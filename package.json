    "url": "https://www.rajatmalik.dev/"
  },
  "contributors": [
    {
      "name": "Rajat Malik",
      "email": "mr.rajatmalik@gmail.com"
    }
  ],
  "maintainers": [
    {
      "name": "Rajat Malik",
      "email": "mr.rajatmalik@gmail.com"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/malikrajat/rm-ng-structure-pdf#readme",
  "repository": {
    "type": "git",
