  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "@angular/common": ">=14.0.0",
    "@angular/core": ">=14.0.0"
  },
  "dependencies": {
    "tslib": "^2.6.0",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1"
  },
  "peerDependenciesMeta": {
    "jspdf": {
      "optional": false
    },
    "html2canvas": {
      "optional": false
    }
