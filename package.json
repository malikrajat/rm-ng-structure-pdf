    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "ng-add": {
    "save": "dependencies"
  },
  "module": "fesm2022/codewithrajat-rm-ng-structure-pdf.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/codewithrajat-rm-ng-structure-pdf.mjs"
    }
  }
